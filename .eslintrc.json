{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 1 }],
      "space-before-function-paren": "off"
    }
}
